<!-- note - contrib initialize files must be separate from build files to allow external inclusion -->
<project>

    <!-- add any specific include directories -->
    <property name="package.DirtySDK.includedirs">
        ${property.value}
        ${package.DirtySDK.dir}/contrib/atgframework/include
        ${package.DirtySDK.dir}/contrib/atgframework/include/${config-system}
    </property>

    <!-- add the library to the list of dirtysock libraries -->
    <property name="package.DirtySDK.libs" value="${property.value};atgframework" />
</project>
